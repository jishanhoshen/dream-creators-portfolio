.projects-area {
  padding: 80px 0px;
  .projects {
    column-count: 3;
    padding: 20px 0px;
    .area-header {
      position: relative;
      display: inline-block;
      &::before {
        position: absolute;
        content: "Portfolio";
        top: 42px;
        left: -26px;
        color: var(--primary-color);
        transform: rotate(-90deg);
        font-size: 18px;
        text-transform: uppercase;
      }
      &::after {
        position: absolute;
        content: "";
        height: 82px;
        width: 2px;
        bottom: 82px;
        left: 17px;
        background-color: var(--primary-color);
      }
      h1 {
        font-size: 32px;
        color: var(--primary-color);
        margin-bottom: 30px;
        padding-left: 42px;
      }
      p {
        color: #7e7e7e;
        margin-bottom: 30px;
        padding-left: 42px;
      }
      .filter {
        padding-left: 42px;
        ul {
          list-style: none;
          margin-bottom: 30px;
          li {
            display: inline-block;
            padding-right: 3px;
            &::after {
              content: "/";
              margin-left: 8px;
            }
            button {
              border: 0;
              background: transparent;
              margin: 0;
              padding: 0;
              cursor: pointer;
              text-transform: capitalize;
            }
          }
