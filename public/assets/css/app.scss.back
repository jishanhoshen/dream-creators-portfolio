.languages-tools {
  background-color: #e5e5e5;
  display: flex;
  align-items: center;
  min-height: 80vh;
  height: fit-content;
  padding: 80px 0px;
  .tools-content {
    display: flex;
    justify-content: space-between;
    .tools-details {
      width: 50%;
      .tool-head {
        position: relative;
        padding-left: 42px;
        &::before {
          position: absolute;
          content: "Our Skills";
          top: 44px;
          left: -27px;
          color: var(--primary-color);
          transform: rotate(-90deg);
          font-size: 18px;
          text-transform: uppercase;
        }
        &::after {
          position: absolute;
          content: "";
          height: 50px;
          width: 2px;
          bottom: 0;
          left: 17px;
          background-color: var(--primary-color);
        }
        h1 {
          font-size: 32px;
          color: var(--primary-color);
          margin-bottom: 30px;
        }
        p {
          color: #7e7e7e;
          margin-bottom: 30px;
        }
      }
      .tool-btn {
        background-color: #fff;
        display: inline-block;
        border-radius: 30px;
        padding: 4px;
        a {
          padding: 10px 20px;
          display: inline-block;
          background-color: white;
          border-radius: inherit;
          text-decoration: none;
          color: var(--primary-color);
          font-weight: 600;
        }
        .active {
          background-color: #814df2;
          color: #fff;
        }
      }
    }
    .skill-tools {
      width: 50%;
      .skill-items {
        display: inline-block;
        width: 100%;
        text-align: center;
      }
      .tool-items {
        display: none;
        width: 100%;
        text-align: center;
      }
      .item {
        background-color: #fff;
        padding: 20px;
        border-radius: 10px;
        height: 80px;
        width: 80px;
        display: inline-flex;
        justify-content: center;
        align-items: center;
        margin: 10px;
        transform: 0.5s;
        cursor: pointer;
        &:hover {
          transform: scale(1.05);
        }
        img {
          width: 40px;
          height: 40px;
          object-fit: contain;
        }
      }
    }
  }
}

.service-area {
  height: 100vh;
  max-height: 700px;
  display: flex;
  align-items: center;
  padding: 40px 0px;
  .service-header {
    h2 {
      text-align: center;
      font-size: 36px;
      color: var(--primary-color);
    }
    p {
      text-align: center;
      margin-bottom: 20px;
      line-height: 26px;
      color: var(--primary-color);
    }
  }
  .service-contain {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    margin: 40px auto 10px;
    .service {
      max-width: 400px;
      p {
        font-size: 14px;
        font-weight: 700;
        padding: 50px 0px 10px;
        text-transform: uppercase;
        font-weight: 400;
        color: var(--primary-color);
      }
      h1 {
        line-height: 42px;
        color: var(--primary-color);
      }
      a {
        text-decoration: none;
        display: inline-block;
        padding: 10px 20px;
        color: var(--primary-color);
        border-radius: 30px;
        margin-top: 20px;
        border: 2px solid var(--primary-color);
        transition: 0.3s;
        &:hover {
          color: #fff;
          background-color: var(--primary-color);
        }
      }
    }
    .services {
      text-align: center;
      .single-service {
        display: inline-block;
        max-width: 345px;
        text-align: center;
        padding: 50px 30px;
        margin: 10px;
        background-color: #f8f9fa;
        cursor: pointer;
        border-radius: 10px;
        transition: 0.3s;
        &:hover {
          background-color: var(--primary-color);
          transform: scale(1.05);
        }
        &:hover i {
          color: #fff;
        }
        &:hover h3 {
          color: #fff;
        }
        &:hover p {
          color: #dbdbdb;
        }
        i {
          color: #495057;
          font-size: 30px;
          margin-bottom: 20px;
        }
        h3 {
          margin-bottom: 15px;
          color: #495057;
        }
        p {
          color: #919191;
        }
      }
    }
  }
}

.contact-area {
  background-color: #fff;
  display: flex;
  align-items: center;
  height: fit-content;
  padding: 100px 0px;
  .contact-content {
    display: flex;
    justify-content: space-between;
    .contact-details {
      width: 50%;
      .contact-head {
        position: relative;
        padding-left: 42px;
        &::before {
          position: absolute;
          content: "Contact";
          top: 42px;
          left: -26px;
          color: var(--primary-color);
          transform: rotate(-90deg);
          font-size: 18px;
          text-transform: uppercase;
        }
        &::after {
          position: absolute;
          content: "";
          height: 60px;
          width: 2px;
          bottom: 0;
          left: 17px;
          background-color: var(--primary-color);
        }
        h1 {
          font-size: 32px;
          color: var(--primary-color);
          margin-bottom: 30px;
        }
        p {
          color: #7e7e7e;
          margin-bottom: 30px;
        }
      }
      .contacts {
        .contact-item {
          display: flex;
          .ico {
            font-size: 18px;
            padding: 5px 10px;
            color: var(--primary-color);
          }
          .details {
            .label {
              font-weight: 600;
              padding: 8px;
              color: var(--primary-color);
            }
            .contact {
              padding: 4px 8px;
              color: #7e7e7e;
              text-decoration: none;
            }
          }
        }
        .socile-link {
          display: flex;
          margin-left: 44px;
          .socile-item {
            background-color: var(--primary-color);
            height: 30px;
            width: 30px;
            display: flex;
            justify-content: center;
            align-items: center;
            margin: 30px 5px 0px;
            border-radius: 50%;
            a {
              text-decoration: none;
              color: #fff;
            }
          }
        }
      }
    }
    .contact-form {
      width: 50%;
      form {
        background-color: #ededed;
        width: 70%;
        margin: auto;
        border-radius: 10px;
        padding: 20px;
      }
      .single-input {
        label {
          text-transform: capitalize;
          font-weight: 600;
          margin: 15px 0px 5px;
          display: inline-block;
          cursor: pointer;
        }
        .input-box {
          background-color: #fff;
          border-radius: 10px;
          display: flex;
          .ico {
            padding: 6px 10px;
            color: #5e5e5e;
            line-height: 26px;
          }
          input,
          textarea {
            background-color: #fff;
            width: 100%;
            border-radius: 0px 10px 10px 0px;
            border: 0;
            margin: 0;
            padding: 10px;
            font-family: "Poppins", sans-serif;
            &:focus-visible {
              outline: -webkit-focus-ring-color auto 0px;
            }
          }
          textarea {
            height: 150px;
          }
        }
      }
      input[type="submit"] {
        color: #fff;
        background-color: var(--primary-color);
        width: 100%;
        border: 0;
        margin: 15px 0px 0px;
        padding: 10px;
        border-radius: 6px;
        cursor: pointer;
      }
    }
  }
}

.footer-area {
