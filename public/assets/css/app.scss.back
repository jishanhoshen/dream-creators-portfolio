@import url("https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap");
:root {
  --cube-size: 100px;
  --cube-slice: 20px;
  --primary-color: #1f1f1f;
}
* {
  margin: 0px;
  padding: 0px;
  box-sizing: border-box;
}
html {
  scroll-behavior: smooth;
}
h3 {
  font-size: 20px;
}
h2 {
  font-size: 24px;
}
h1 {
  font-size: 36px;
}
.extra-large {
  font-size: 74px;
}
.large {
  font-size: 50px;
}
.small {
  font-size: 12px;
}
body {
  font-size: 14px;
  font-family: "Poppins", sans-serif;
  color: var(--primary-color);
  // overflow: hidden;
  background-repeat: no-repeat;
  background-size: inherit;
  background-position: top center;
}
.container {
  width: 1170px;
  margin: auto;
  padding: 0px 10px;
}
.preloader {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 10;
  background-color: #000;
}

.floating-left {
  position: fixed;
  bottom: 50px;
  left: 20px;
  z-index: 5;
  .contact {
    text-decoration: none;
    color: #fff;
    background-color: #814df2;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    margin: 10px 0px;
    box-shadow: 0px 4px 4px #00000099;
    border: 3px solid #814df2;
    &:hover {
      border: 3px solid #fff;
    }
  }
}
.floating-right {
  position: fixed;
  bottom: 50px;
  right: 20px;
  z-index: 5;
  .scrallup {
    opacity: 0;
  }
  .scrallup,
  .live-chat {
    text-decoration: none;
    color: #fff;
    background-color: #814df2;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    margin: 10px 0px;
    box-shadow: 0px 4px 4px #00000099;
    transition: 0.5s;
    cursor: pointer;
    border: 3px solid #814df2;
    &:hover {
      border: 3px solid #fff;
    }
  }
}

header {
  position: fixed;
  top: 15px;
  left: 0;
  right: 0;
  z-index: 5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  .logo {
    background-color: #814df2;
    padding: 0px 20px 0px calc(100vw - 90%);
    border-radius: 0px 30px 30px 0px;
    a {
      text-decoration: none;
      display: inline;
      img {
        width: 100%;
        height: fit-content;
      }
      h2 {
        color: #fff;
        padding: 10px 0px;
      }
    }
  }
  .header-menu {
    background-color: #814df2;
    padding: 0px calc(100vw - 90%) 0px 20px;
    border-radius: 30px 0px 0px 30px;
    ul {
      li {
        list-style: none;
        display: inline-block;
        a {
          text-decoration: none;
          display: block;
          padding: 15px;
          margin: 0px;
          font-size: 16px;
          color: #fff;
          // &:hover {
          //     border-bottom: 2px solid var(--primary-color);
          // }
        }
        // a.active {
        //     border-bottom: 2px solid var(--primary-color);
        // }
      }
    }
  }
}

.hero-area {
  height: 100vh;
  max-height: 700px;
  .hero-contain {
    display: flex;
    justify-content: space-evenly;
    align-items: center;
    height: inherit;
    max-height: inherit;
    .content {
      h1 {
        text-transform: capitalize;
        font-weight: 500;
        color: #7e2df5;
      }
      a {
        transition: 0.3s;
        text-decoration: none;
        background: #ffffff;
        border: 2px solid #2175fb;
        color: #2175fb;
        border-radius: 30px;
        padding: 10px 20px;
        display: inline-block;
        margin-top: 25px;
        &:hover {
          color: #fff;
          background-color: #2175fb;
        }
      }
    }
    // h1 {
    //     margin: 0px;
    //     font-size: 42px;
    //     font-weight: 400;
    //     text-align: center;
    //     padding-top: 300px;
    //     color: var(--primary-color);
    // }
    // p {
    //     font-size: 14px;
    //     text-align: center;
    //     padding-bottom: 300px;
    //     color: var(--primary-color);
    // }
  }
}

.exprence-area {
  margin: 40px auto;
  .container {
    .experience {
      display: flex;
      justify-content: space-evenly;
      .ex-box {
        display: inline-flex;
        align-items: center;
        background: #ffffff;
        box-shadow: 0px 0px 30px #00000040;
        border-radius: 20px;
        cursor: pointer;
        transition: 0.5s;
        &:hover {
          transform: scale(1.05);
        }
        .ico {
          background-color: #6752ed;
          border-radius: 50%;
          color: #fff;
          font-size: 26px;
          width: 60px;
          height: 60px;
          display: flex;
          align-items: center;
          justify-content: center;
          margin: 35px 40px;
        }
        .content {
          margin-right: 40px;
          text-align: center;
          h1 {
            font-weight: 600;
            font-size: 42px;
            line-height: 70%;
            margin-bottom: 10px;
          }
          p {
            font-weight: 600;
            font-size: 16px;
            text-transform: capitalize;
          }
        }
      }
    }
  }
}

.dashboard {
  background-color: #f7f6f2;
  .mainArea {
    display: flex;
    .navbar-small {
      width: 66px !important;
      border-radius: 30px !important;
      .nav-header {
        .company {
          .logo {
            display: none;
          }
        }
      }
    }
    .navbar {
      background-color: #ffffff;
      display: inline-flex;
      flex-direction: column;
      justify-content: space-between;
      width: 250px;
      height: calc(100vh - 12px);
      margin: 6px;
      padding: 20px 10px 0px;
      border-radius: 0px 30px 30px 0px;
      box-shadow: 0px 0px 20px #cdcdcd;
      transition: 0.6s;
      .nav-header {
        .company {
          margin-bottom: 40px;
          height: 30px;
          .logo {
            color: #000;
            text-align: center;
          }
        }
        .nav-list {
          padding: 0px 10px;
          .nav-item {
            list-style: none;
            a {
              display: flex;
              align-items: center;
              padding: 13px 0px;
              margin: 1px;
              color: #000000;
              text-decoration: none;
              text-transform: capitalize;
              i {
                padding-right: 20px;
                font-size: 22px;
                width: 50px;
              }
            }
            a.active {
              color: #039c83;
            }
          }
        }
      }
      .nav-footer {
        .nav-footer-content {
          color: #000;
          margin-bottom: 20px;
          padding: 0px 10px;
          a {
            text-decoration: none;
          }
        }
        a.navSwitch {
          color: #000;
          font-size: 20px;
          margin-bottom: 20px;
          display: block;
          padding-left: 10px;
        }
      }
    }
    .contentArea {
      width: 100%;
      .header {
        height: 60px;
        background-color: #f7f6f2;
        display: flex;
        justify-content: flex-end;
        align-items: center;
        margin: 6px 20px 0px;
        .search {
          color: #000;
          form {
            display: flex;
            align-items: center;
            input {
              margin: 0;
              padding: 0;
              border: 0;
              &:focus-visible {
                outline: -webkit-focus-ring-color auto 0px;
              }
              &::-webkit-search-decoration,
              &::-webkit-search-cancel-button,
              &::-webkit-search-results-button,
              &::-webkit-search-results-decoration {
                display: none;
              }
              &::placeholder {
                color: #a8a8a8;
              }
            }
            input[type="search"] {
              font-size: 16px;
              padding: 8px;
              width: 250px;
              background-color: #f7f6f2;
              border-bottom: 1px solid #a0a0a0;
              transition: 0.5s;
              display: none;
            }
            input[type="search"].active {
              display: block;
            }
            .search-ico {
              font-size: 20px;
              padding: 5px 12px;
              cursor: pointer;
            }
          }
        }
        .notification {
          color: #000;
          font-size: 20px;
          margin: 0px 15px;
        }
        .profile {
          color: #000;
          margin: 0px 20px;
          height: 40px;
          width: 40px;
          position: relative;
          img {
            width: 40px;
            height: 40px;
            object-fit: cover;
            object-position: top;
            border-radius: 50%;
            border: 3px solid #fff;
            box-shadow: 0px 0px 20px #cdcdcd;
            cursor: pointer;
          }
          .dropdown {
            background-color: #ffffff;
            width: 200px;
            position: absolute;
            right: -10px;
            box-shadow: 0px 0px 20px #cdcdcd;
            border-radius: 10px;
            padding: 7px;
            top: 55px;
            display: none;
            z-index: 1;
            &::before {
              content: "";
              position: absolute;
              background: white;
              height: 15px;
              width: 15px;
              top: -7px;
              right: 25px;
              transform: rotate(45deg);
              z-index: -1;
            }
            ul {
              display: block;
              li {
                list-style: none;
                a {
                  text-decoration: none;
                  color: #000;
                  display: block;
                  padding: 10px 20px;
                  display: flex;
                  justify-content: space-between;
                  align-items: center;
                  &:hover {
                    background-color: #efefef;
                  }
                  i {
                    padding: 0px 10px;
                  }
                }
              }
            }
          }
        }
      }
      .mainContainer {
        background-color: #ffffff;
        margin: 20px;
        padding: 20px;
        height: calc(100vh - (66px + (20px * 2)));
        border-radius: 30px 0px 0px 30px;
        box-shadow: 0px 0px 20px #cdcdcd;
        .content-area {
          display: none;
        }
        .content-area.active {
          display: block;
          .project-submit-area {
            input,
            textarea,
            #editor {
              padding: 8px 20px;
              border: 1px solid #d9d9d9;
              border-radius: 4px;
              font-size: 16px;
              font-family: "Poppins", sans-serif;
              &:focus-visible {
                outline: -webkit-focus-ring-color auto 0px;
              }
            }
            .title {
              margin: 10px;
              h2 {
                margin: 10px 0px;
              }
              input[type="text"] {
                width: 660px;
                margin-right: 30px;
              }
              input[type="submit"] {
                background-color: var(--primary-color);
                color: #fff;
              }
            }
            .project-inputs-area {
              display: flex;
              .leftside {
                width: 750px;
              }
              .rightside {
                width: 300px;
                input,
                textarea,
                tags {
                  margin: 5px 0px;
                  width: 100%;
                }
                label {
                  font-weight: 600;
                  margin-top: 6px;
                  display: inline-block;
                }
              }
            }
          }
        }
      }
    }
  }
}
.tagify__input::before {
  line-height: 1.3em !important;
}
.editor {
  display: flex;
  justify-content: center;
  flex-direction: column;
  margin: 10px 30px 10px 10px;
  .editor-head {
    .editor-tools {
      display: flex;
      .tool {
        display: flex;
        align-items: center;
        font-size: 16px;
        margin: 3px;
        button {
          border: 0;
          background-color: transparent;
          padding: 8px 10px;
          cursor: pointer;
        }
        select {
          font-size: 14px;
          border: 1px solid #ddd;
          padding: 5px;
          border-radius: 4px;
          cursor: pointer;
        }
        input[type="color"] {
          padding: 0px !important;
          width: 24px;
          cursor: pointer;
        }
      }
    }
  }
  #editor {
    width: 100%;
    height: 300px;
    overflow-y: scroll;
  }
}
.blog {
  .postarea {
    width: 550px;
    margin: auto;
    .singlepost {
      width: inherit;
      padding: 40px 0px;
      img {
        width: inherit;
        height: 250px;
        object-fit: cover;
      }
      p {
        padding: 10px 0px;
        color: #7e7e7e;
      }
      h1 {
        text-transform: capitalize;
        margin-bottom: 20px;
      }
      .postbody {
        height: 100px;
        display: -webkit-box;
        -webkit-line-clamp: 4;
        -webkit-box-orient: vertical;
        overflow: hidden;
        p {
          padding: 0px;
          line-height: 26px;
          color: rgb(56, 56, 56);
        }
      }
      a {
        text-decoration: none;
        padding: 8px 0px;
        display: inline-block;
      }
    }
  }
  .singlearea {
    .postbody {
      display: block !important;
      height: fit-content !important;
    }
    a {
      display: none !important;
    }
  }
}

